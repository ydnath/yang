  }

  identity end-dx4-next-usid {
    base srv6-endpoint-type;
    description
      "End.DX4 with NEXT-CSID";
  }

  identity end-dt6-next-usid {
    base srv6-endpoint-type;
    description
      "End.DT6 with NEXT-CSID";
  }

  identity end-dt4-next-usid {
    base srv6-endpoint-type;
    description
      "End.DT4 with NEXT-CSID";
  }

  identity end-dt46-next-usid {
    base srv6-endpoint-type;
    description
      "End.DT46 with NEXT-CSID";
  }

  identity end-dx2-next-usid {
    base srv6-endpoint-type;
    description
      "End.DX2 with NEXT-CSID";
  }

  identity end-dx2v-next-usid {
    base srv6-endpoint-type;
    description
      "End.DX2V with NEXT-CSID";
  }

