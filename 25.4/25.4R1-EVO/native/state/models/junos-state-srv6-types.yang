  identity srv6-counter-type {
    description
      "Base identity from which SRv6 counter types are derived.";
  }

  identity local-sid-ingress {
    base srv6-counter-type;
    description
      "Count packets and bytes traffic that matched that SID and was
       processed successfully";
    reference
      "RFC 8986";
  }

  identity remote-locator-ingress {
    base srv6-counter-type;
    description
      "Count packets and bytes traffic that matched that locator and was
       processed successfully";
  }

  identity remote-locator-egress {
    base srv6-counter-type;
    description
      "Count packets and bytes traffic that matched that locator and was
       processed successfully";
  }

  identity srv6-sid-owner-type {
    description
      "Base identity from which SID owner types are derived.";
  }

  identity isis {
    base srv6-sid-owner-type;
    description
      "ISIS";
  }

  identity ospf {
    base srv6-sid-owner-type;
    description
      "OSPFv3";
  }

  identity bgp {
    base srv6-sid-owner-type;
    description
      "BGP";
  }

  identity evpn {
    base srv6-sid-owner-type;
    description
