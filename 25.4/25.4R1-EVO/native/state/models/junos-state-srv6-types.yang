  identity end-t-psp-usp-usd {
    base srv6-endpoint-type;
    description
      "Endpoint with specific IPv6 table lookup (variant: PSP and USP
       and USD).";
    reference
      "RFC 8986";
  }

  identity end-with-next-only-usid {
    base srv6-endpoint-type;
    description
      "End with NEXT-ONLY-CSID";
  }

  identity end-with-next-usid {
    base srv6-endpoint-type;
   description
      "End with NEXT-CSID";
  }

  identity end-with-next-psp-usid {
    base srv6-endpoint-type;
    description
      "End with NEXT-CSID, PSP";
  }

  identity end-with-next-usp-usid {
    base srv6-endpoint-type;
    description
      "End with NEXT-CSID, USP";
  }

  identity end-with-next-psp-usp-usid {
    base srv6-endpoint-type;
    description
      "End with NEXT-CSID, PSP & USP";
  }

  identity end-with-next-usd-usid {
    base srv6-endpoint-type;
    description
      "End with NEXT-CSID, USD";
  }

  identity end-with-next-psp-usd-usid {
    base srv6-endpoint-type;
    description
      "End with NEXT-CSID, PSP & USD";
  }

  identity end-with-next-usp-usd-usid {
    base srv6-endpoint-type;
    description
      "End with NEXT-CSID, USP & USD";
  }

  identity end-with-next-psp-usp-usd-usid {
    base srv6-endpoint-type;
    description
      "End with NEXT-CSID, PSP, USP & USD";
  }

  identity end-x-with-next-only-usid {
    base srv6-endpoint-type;
    description
      "End.X with NEXT-ONLY-CSID";
  }

  identity end-x-with-next-usid {
    base srv6-endpoint-type;
    description
      "End.X with NEXT-CSID";
  }

  identity end-x-with-next-psp-usid {
    base srv6-endpoint-type;
    description
      "End.X with NEXT-CSID, PSP";
  }

  identity end-x-with-next-usp-usid {
    base srv6-endpoint-type;
    description
      "End.X with NEXT-CSID, USP";

  }

  identity end-x-with-next-psp-usp-usid {
    base srv6-endpoint-type;
    description
      "End.X with NEXT-CSID, PSP & USP";
  }

  identity end-x-with-next-usd-usid {
